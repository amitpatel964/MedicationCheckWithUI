<BorderPane fx:controller="com.amitpatel.medicationcheck.ControllerNewWindow1" xmlns:fx="http://javafx.com/fxml">

    <center>
        <GridPane alignment="CENTER" hgap="10" vgap="5">
            <Label fx:id="medicineAdded" BorderPane.alignment="TOP_CENTER" GridPane.columnIndex="0" GridPane.rowIndex="0">
                <font>
                    <Font size="15"/>
                </font>
            </Label>
            <Button fx:id="buttonDone" text="Close" onAction="#helpFinish" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
        </GridPane>
    </center>

</BorderPane>
